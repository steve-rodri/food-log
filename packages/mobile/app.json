{
  "expo": {
    "name": "food-log",
    "slug": "food-log",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "rnCliPath": "../../node_modules/react-native/local-cli/cli.js",
    "nodeModulesPath": "../../",
    "entryPoint": "./index.js",
    "packagerOpts": {
      "config": "./rn-cli.config.js"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
